<?xml version='1.0' encoding='UTF-8'?>
<object xmlns="http://n2oapp.net/framework/config/schema/object-4.0">
    <fields>
        <field id="id"/>
        <field id="fileName"/>
        <field id="url"/>
    </fields>

    <operations>
        <operation id="delete">
            <invocation>
                <java method="delete">
                    <arguments>
                        <argument name="id" type="primitive"/>
                    </arguments>
                    <spring bean="fileStorageController"/>
                </java>
            </invocation>
            <in>
                <field id="id"/>
            </in>
        </operation>

        <operation id="submit">
            <invocation>
                <java method="submit">
                    <arguments>
                        <argument name="fileUpload.id" type="class"/>
                    </arguments>
                    <spring bean="fileStorageController"/>
                </java>
            </invocation>
            <in>
                <field id="fileUpload*.id" mapping="[0]" domain="integer[]"/>
            </in>
        </operation>
    </operations>
</object>