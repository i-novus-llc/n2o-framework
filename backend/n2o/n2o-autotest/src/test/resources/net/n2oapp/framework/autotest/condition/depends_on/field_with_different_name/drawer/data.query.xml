<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="net/n2oapp/framework/autotest/condition/depends_on/field_with_different_name/drawer/data.json"
              operation="findAll"/>
    </list>
    <count>
        <test file="data.json" operation="count"/>
    </count>

    <fields>
        <field id="id" name="id">
            <select/>
            <filters>
                <eq filter-id="id"/>
            </filters>
            <sorting/>
        </field>
        <field id="region" name="Регион">
            <select/>
            <filters>
                <eq filter-id="region.id" mapping="['region.id']">region.id:eq:region.id</eq>
            </filters>
        </field>
    </fields>
</query>
