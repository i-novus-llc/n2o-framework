<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
      name="Browser storage. Восстановление модели из указанного кэша">
    <datasources>
        <datasource id="ds1"/>
    </datasources>
    <regions>
        <form datasource="ds1">
            <fields>
                <input-text id="input"/>
            </fields>

            <toolbar>
                <button label="Кнопка">
                    <invoke
                            operation-id="create"
                            object-id="test"
                            refresh-on-success="false"
                    />
                    <clear datasource="ds1" model="resolve"/>
                </button>
            </toolbar>
        </form>
    </regions>
</page>