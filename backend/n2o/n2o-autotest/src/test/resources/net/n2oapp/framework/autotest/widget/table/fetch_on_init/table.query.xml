<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="net/n2oapp/framework/autotest/widget/table/fetch_on_init/table.json"/>
    </list>

    <fields>
        <field id="id">
            <select/>
        </field>
        <field id="name">
            <select/>
            <filters>
                <eq filter-id="name"/>
            </filters>
        </field>
        <field id="surname">
            <select/>
            <filters>
                <eq filter-id="surname"/>
            </filters>
        </field>
    </fields>
</query>
