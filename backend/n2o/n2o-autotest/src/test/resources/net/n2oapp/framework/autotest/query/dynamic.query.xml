<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="net/n2oapp/framework/autotest/query/dynamic.json"/>
    </list>

    <fields>
        <field id="id">
            <select/>
            <filters>
                <eq/>
            </filters>
        </field>
        <field id="versionId">
            <select/>
            <filters>
                <eq/>
            </filters>
        </field>
        <field id="name">
            <select default-value="${table}"/>
        </field>
    </fields>
</query>