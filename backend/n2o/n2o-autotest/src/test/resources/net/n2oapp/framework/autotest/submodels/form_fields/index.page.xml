<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-3.0">
    <form>
        <fields>
            <input-text id="name"/>
        </fields>
        <toolbar>
            <button label="Open" model="filter">
                <open-page page-id="new_page" route="/open" upload="defaults">
                    <query-param name="gender_param" value="1"/>
                    <query-param name="type_param" value="2"/>
                </open-page>
            </button>
        </toolbar>
    </form>
</simple-page>