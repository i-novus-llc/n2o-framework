<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="net/n2oapp/framework/autotest/widget/table/filters/form_as_filter/test.json"/>
    </list>

    <fields>
        <field id="id">
            <select/>
        </field>
        <field id="name">
            <select/>
        </field>
        <field id="type">
            <select/>
            <filters>
                <eq filter-id="type"/>
            </filters>
        </field>
        <field id="period.id">
            <filters>
                <eq filter-id="period.id"/>
            </filters>
        </field>
    </fields>
</query>
