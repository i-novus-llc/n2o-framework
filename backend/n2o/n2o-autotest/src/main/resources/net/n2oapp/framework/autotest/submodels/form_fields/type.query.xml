<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list>
        <test file="type.json" operation="findAll"/>
    </list>
    <count>
        <test file="type.json" operation="count"/>
    </count>
    <unique filters="id">
        <test file="type.json" operation="findOne"/>
    </unique>
    <filters>
        <eq filter-id="id" field-id="id"/>
        <eq filter-id="name" field-id="name"/>
    </filters>
    <fields>
        <field id="id"/>
        <field id="name"/>
    </fields>
</query>