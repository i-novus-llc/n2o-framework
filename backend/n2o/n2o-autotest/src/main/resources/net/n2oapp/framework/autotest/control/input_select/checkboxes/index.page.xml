<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0">
    <form>
        <fields>
            <input-select id="select" label="InputSelect" type="checkboxes">
                <options>
                    <option name="One" id="one"/>
                    <option name="Two" id="two"/>
                    <option name="Three" id="three"/>
                </options>
            </input-select>
            <input-select id="select3" label="InputSelect3" enabled-field-id="isDisabled" type="checkboxes">
                <options>
                    <option name="One" id="one"/>
                    <option name="Two" id="two"/>
                    <option name="Three" id="three" isDisabled="false"/>
                    <option name="Four" id="four" isDisabled="true"/>
                </options>
            </input-select>
        </fields>
    </form>
</simple-page>