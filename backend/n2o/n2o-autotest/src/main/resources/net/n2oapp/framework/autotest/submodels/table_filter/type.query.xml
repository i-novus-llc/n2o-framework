<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="type.json" operation="findAll"/>
    </list>
    <count>
        <test file="type.json" operation="count"/>
    </count>
    <unique filters="id">
        <test file="type.json" operation="findOne"/>
    </unique>

    <fields>
        <field id="id">
            <select/>
            <filters>
                <eq filter-id="id"/>
            </filters>
        </field>
        <field id="name">
            <select/>
            <filters>
                <eq filter-id="name"/>
            </filters>
        </field>
    </fields>
</query>