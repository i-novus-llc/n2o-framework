<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-3.0" name="Название страницы" route="/pageRoute">
    <form>
        <fields>
            <file-upload label="FileUpload1" id="fileUpload1" multi="true"
                         label-field-id="fileName" value-field-id="id" url-field-id="url"
                         upload-url="http://localhost:0/files"
                         delete-url="http://localhost:0/files"
            />

            <file-upload label="FileUpload2" id="fileUpload2" multi="false"
                         label-field-id="fileName" value-field-id="id" url-field-id="url"
                         upload-url="/files"
                         delete-url="/files"
                         accept=".json"
            />

            <file-upload label="FileUpload3" id="fileUpload3" multi="true"
                         label-field-id="fileName" value-field-id="id" url-field-id="url"
                         upload-url="/files"
                         delete-url="/files"
            />
        </fields>
    </form>
</simple-page>
