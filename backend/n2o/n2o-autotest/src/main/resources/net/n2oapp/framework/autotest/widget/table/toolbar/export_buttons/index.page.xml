<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
      name="Страница для автотестов по проверке кнопки 'экспортировать' в таблице">
    <datasources>
        <datasource id="ds1" query-id="data" size="3"/>
    </datasources>

    <regions>
        <table datasource="ds1" checkboxes="true" selection="none">
            <columns>
                <column text-field-id="id_"/>
                <column text-field-id="id_ips"/>
                <column text-field-id="name"/>
                <column text-field-id="region"/>
            </columns>
            <toolbar place="topRight">
                <group generate="export"/>
            </toolbar>
        </table>
    </regions>
</page>
