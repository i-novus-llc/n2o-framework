<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
      name="Страница для автотеста по проверке работы действия clear после другого действия"
>
    <regions>
        <form id="form">
            <toolbar id="toolbar" place="topLeft" >
                <button label="clear"
                        disable-on-empty-model="false"
                        validate="false"
                >
                    <clear />
                </button>
                <button
                        label="copy"
                        disable-on-empty-model="false"
                        validate="false"
                >
                    <set-value to="name">
                        "value"
                    </set-value>
                </button>
            </toolbar>

            <fields>
                <input-text
                        id="name"
                        required="true"
                        default-value="Привет, Мир!"
                        label="Исходные данные"
                />
            </fields>
        </form>
    </regions>
</page>
