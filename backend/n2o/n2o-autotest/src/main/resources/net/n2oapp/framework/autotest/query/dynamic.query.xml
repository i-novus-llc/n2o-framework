<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list>
        <test file="dynamic.json"/>
    </list>

    <filters>
        <eq field-id="id"/>
        <eq field-id="versionId"/>
    </filters>

    <fields>
        <field id="id"/>
        <field id="versionId"/>
        <field id="name" default-value="${table}"/>
    </fields>
</query>