<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-3.0">
    <table query-id="test">
        <columns>
            <column id="c1" label="Text" text-field-id="name">
                <link icon="{icon}" url="/{url}"/>
            </column>
            <column id="c2" label="Text and icon" text-field-id="name">
                <link icon="{icon}" type="iconAndText" url="/{url}"/>
            </column>
            <column id="c3" label="Icon" text-field-id="name">
                <link icon="{icon}" type="icon"  url="/{url}"/>
            </column>
        </columns>
    </table>
</simple-page>