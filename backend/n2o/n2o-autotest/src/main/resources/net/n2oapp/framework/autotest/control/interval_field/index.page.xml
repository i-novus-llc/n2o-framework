<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0">
    <form>
        <fields>
            <interval-field id="inputText" label="Интервал">
                <begin>
                    <input-text id="beginInputText" domain="integer" default-value="5"/>
                </begin>
                <end>
                    <input-text id="endInputText" domain="integer" default-value="10" max="10"/>
                </end>
            </interval-field>
            <interval-field id="dataTime" label="Дата">
                <begin>
                    <date-time id="dataBegin" default-value="1999-11-21T00:00:00" date-format="DD.MM.YYYY"/>
                </begin>
                <end>
                    <date-time id="dataEnd" date-format="DD.MM.YYYY"/>
                </end>
            </interval-field>
        </fields>
    </form>
</simple-page>