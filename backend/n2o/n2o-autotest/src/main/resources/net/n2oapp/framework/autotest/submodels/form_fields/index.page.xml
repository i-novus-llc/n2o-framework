<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0">
    <form>
        <toolbar>
            <button label="Open" model="filter">
                <open-page page-id="new_page" route="/open">
                    <datasources>
                        <datasource default-values-mode="defaults"/>
                    </datasources>
                    <params>
                        <query-param name="gender_param" value="1"/>
                        <query-param name="type_param" value="2"/>
                    </params>
                </open-page>
            </button>
        </toolbar>
        <fields>
            <input-text id="name"/>
        </fields>
    </form>
</simple-page>