<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
             name="Страница для проверки изменяемоисти состояния чекбокса">
    <form>
        <fields>
            <checkbox
                    id="isNull"
                    label="Init null"
                    unchecked="null"
            />
            <input-text id="isNull"/>

            <checkbox
                    id="isFalse"
                    label="Init false"
                    unchecked="false"
            />
            <input-text id="isFalse"/>
        </fields>
    </form>
</simple-page>
