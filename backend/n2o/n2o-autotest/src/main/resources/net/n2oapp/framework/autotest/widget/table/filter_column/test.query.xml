<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="test.json"/>
    </list>

    <fields>
        <field id="id">
            <select/>
            <filters>
                <eq filter-id="id"/>
            </filters>
        </field>
        <field id="name">
            <select/>
            <filters>
                <like filter-id="name">name :like :name</like>
            </filters>
        </field>
        <field id="type">
            <select/>
            <filters>
                <eq filter-id="type.id">type :eq :type.id</eq>
            </filters>
        </field>
        <field id="birthday">
            <select/>
            <filters>
                <more filter-id="birthday.begin">birthday :more :birthday.begin</more>
                <less filter-id="birthday.end">birthday :less :birthday.end</less>
            </filters>
        </field>
    </fields>
</query>