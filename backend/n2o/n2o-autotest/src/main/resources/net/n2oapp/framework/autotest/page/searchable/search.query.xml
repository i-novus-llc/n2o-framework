<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="search.json" operation="findAll"/>
    </list>
    <count>
        <test file="search.json" operation="count"/>
    </count>

    <fields>
        <field id="id">
            <select/>
        </field>
        <field id="name">
            <select/>
            <filters>
                <like filter-id="name"/>
            </filters>
        </field>
        <field id="date">
            <select/>
        </field>
    </fields>
</query>
