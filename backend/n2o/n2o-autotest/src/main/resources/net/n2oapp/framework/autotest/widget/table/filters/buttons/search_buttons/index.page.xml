<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
             name="Автотест кнопок поиска в фильтров таблицы">
    <table>
        <datasource query-id="test"/>
        <filters>
            <input-text id="name"/>
            <input-text id="type"/>
            <search-buttons id="searchButtons" search-label="SearchB" reset-label="ClearB" clear-ignore="type"/>
        </filters>
        <columns>
            <column text-field-id="name"/>
            <column text-field-id="type"/>
        </columns>
    </table>
</simple-page>