<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list>
        <test file="tree.json"/>
    </list>
    <count>
        <test file="tree.json" operation="count"/>
    </count>
    <filters>
        <eq filter-id="id" field-id="id"/>
        <eq filter-id="name" field-id="name"/>
        <eq filter-id="parent" field-id="parent"/>
    </filters>
    <fields>
        <field id="id"/>
        <field id="name"/>
        <field id="icon"/>
        <field id="badge"/>
        <field id="status"/>
        <field id="parent"/>
        <field id="image"/>
    </fields>
</query>