<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-3.0">
    <form>
        <fields>
            <multi-set id="members1" can-remove="false">
                <input-text id="name"/>
            </multi-set>
            <multi-set id="members2" children-label="Участник {(index + 1)}">
                <input-text id="name"/>
            </multi-set>
            <multi-set id="members3" remove-all-label="Удалить всех участников" can-remove-all="true">
                <input-text id="name"/>
            </multi-set>
            <multi-set id="members4" can-remove-all="true" can-remove-first="true">
                <input-text id="name"/>
            </multi-set>
        </fields>
    </form>
</simple-page>