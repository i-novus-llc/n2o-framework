<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
             xmlns:ts="http://n2oapp.net/framework/config/schema/table-settings-1.0">
    <table>
        <datasource query-id="data"/>
        <columns>
            <column text-field-id="id"/>
            <multi-column id="id_multi1" label="multi1">
                <column text-field-id="name"/>
                <column text-field-id="type"/>
            </multi-column>
            <multi-column id="id_multi2" label="multi2">
                <column text-field-id="status"/>
                <column text-field-id="region"/>
                <multi-column label="nestedmulti">
                    <column text-field-id="city"/>
                    <column text-field-id="country"/>
                </multi-column>
            </multi-column>
            <multi-column id="id_multi3" label="multi3">
                <column text-field-id="address"/>
                <column text-field-id="phone"/>
                <column text-field-id="email"/>
            </multi-column>
            <multi-column id="id_multi4" label="multi4">
                <column text-field-id="gender"/>
                <column text-field-id="birthdate"/>
            </multi-column>
            <dnd-column>
                <column text-field-id="dnd0"/>
                <column text-field-id="dnd1"/>
                <column text-field-id="dnd2"/>
            </dnd-column>
        </columns>
        <toolbar place="bottomLeft">
            <ts:columns locked="id, id_multi1, id_multi2, dnd0" default-value="id_multi3, dnd1"/>
        </toolbar>
    </table>
</simple-page>