<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0" name="Префильтры в AutoComplete">
    <form>
        <fields>
            <select id="type">
                <default-value name="type1" id="1"/>
                <options>
                    <option name="type1" id="1"/>
                    <option name="type2" id="2"/>
                </options>
            </select>
            <auto-complete id="auto" query-id="test" value-field-id="name" search-filter-id="name">
                <filters>
                    <eq field-id="type" value="{type.name}"/>
                </filters>
            </auto-complete>
        </fields>
    </form>
</simple-page>