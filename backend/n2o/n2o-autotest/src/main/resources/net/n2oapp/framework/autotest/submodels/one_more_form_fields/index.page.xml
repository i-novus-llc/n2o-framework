<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
             name="Страница для автотест проверяющего сабмодели при более одной формы на странице">
    <table>
        <datasource query-id="test"/>
        <columns>
            <column text-field-id="id"/>
            <column text-field-id="name"/>
        </columns>
        <toolbar>
            <button label="Изменить">
                <open-page page-id="test" route="/:pid/card">
                    <params>
                        <path-param name="pid" value="{id}"/>
                    </params>
                </open-page>
            </button>
        </toolbar>
    </table>
</simple-page>


