<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list>
        <test file="testList.json"/>
    </list>
    <count>
        <test file="testList.json" operation="count"/>
    </count>
    <unique>
        <test file="testList.json" operation="findOne"/>
    </unique>
    <filters>
        <eq field-id="id"/>
    </filters>
    <fields>
        <field id="id"/>
        <field id="leftTop"/>
        <field id="leftBottom"/>
        <field id="header"/>
        <field id="body"/>
        <field id="subHeader"/>
        <field id="rightTop"/>
        <field id="rightBottom"/>
        <field id="extra"/>
    </fields>
</query>