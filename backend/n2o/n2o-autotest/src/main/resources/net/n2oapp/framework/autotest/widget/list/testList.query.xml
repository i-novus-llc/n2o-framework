        <test file="testList.json"/>
    </list>
    <unique>
        <test file="testList.json" operation="findOne"/>
    </unique>
    <count>
        <test file="testList.json" operation="count"/>
    </count>
    <fields>
        <field id="id">
            <select/>
            <filters>
                <eq/>
            </filters>
        </field>
        <field id="leftTop">
            <select/>
        </field>
        <field id="leftBottom">
            <select/>
        </field>
        <field id="header">
            <select/>
        </field>
        <field id="body">
            <select/>
        </field>
        <field id="subHeader">
            <select/>
        </field>
        <field id="rightTop">
            <select/>
        </field>
        <field id="rightBottom">
            <select/>
        </field>
        <field id="extra">
            <select/>
        </field>
    </fields>
</query>







