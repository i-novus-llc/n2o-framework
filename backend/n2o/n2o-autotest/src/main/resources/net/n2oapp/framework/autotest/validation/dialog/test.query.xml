<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list result-mapping="#this" count-mapping="#this.length">
        <java method="findAll">
            <spring bean="dialogService"/>
        </java>
    </list>

    <fields>
        <field id="id">
            <select/>
        </field>
        <field id="name">
            <select/>
        </field>
        <field id="age">
            <select/>
        </field>
    </fields>
</query>
