<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-4.0">
    <datasources>
        <datasource id="ds1"/>
        <datasource id="ds2"/>
    </datasources>
    <toolbar>
        <button id="b1">
            <edit-list operation="create" datasource="ds1" model="multi" list-field-id="groups[{idx}]"
