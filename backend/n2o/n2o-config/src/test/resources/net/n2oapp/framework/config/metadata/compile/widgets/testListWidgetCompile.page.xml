<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0">
    <list>
        <datasource size="5" query-id="utBlank" object-id="utBlank"/>
        <toolbar>
            <button>
                <invoke operation-id="delete"/>
            </button>
        </toolbar>
        <pagination src="pagingSrc" prev="true" next="true"/>
        <content>
            <left-top text-field-id="test"/>
            <header text-field-id="test">
                <progress color="test"/>
            </header>
            <sub-header>
                <checkbox enabled="{name!='Мария'}"/>
            </sub-header>
            <body>
                <badge visible="{1 == 2}"/>
            </body>
            <right-top text-field-id="rightTopId">
                <link>
                    <a href="test"/>
                </link>
            </right-top>
            <right-bottom>
                <icon class="test"/>
            </right-bottom>
            <extra>
                <toolbar>
                    <button>
                        <invoke operation-id="create"/>
                    </button>
                </toolbar>
            </extra>
        </content>
    </list>
</simple-page>