<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list>
        <test/>
    </list>

    <filters>
        <eq field-id="id"/>
        <eq field-id="versionId"/>
        <eq filter-id="name" field-id="name"/>
    </filters>

    <fields>
        <field id="id"/>
        <field id="versionId" default-value="${version}"/>
        <field id="name" default-value="${name}"/>
    </fields>
</query>