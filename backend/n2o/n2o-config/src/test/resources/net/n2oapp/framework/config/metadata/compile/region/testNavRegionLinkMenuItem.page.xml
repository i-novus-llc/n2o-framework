<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-4.0">
    <datasources>
        <datasource id="ds"/>
    </datasources>
    <regions>
        <nav datasource="ds" model="datasource">
            <link
                    label="test label"
                    href="http://test.com"
                    icon="fa fa-bell"
                    icon-position="right"
                    target="self"
                    visible="{1==2}"
                    enabled="{a==b}"
                    model="filter"
                    class="class"
                    style="background-color: blue">
                <path-param name="id1" value="{id1}" datasource="ds1" model="edit"/>
                <query-param name="id2" value="{id2}"/>
            </link>
            <group>
                <link
                        label="sublabel"
                        href="/person/:person_id/docs"
                        icon="fa fa-home"
                        visible="false"
                        class="class1"
                        style="color: red">
                    <query-param name="id" value="{id}"/>
                </link>
            </group>
        </nav>
    </regions>
</page>
