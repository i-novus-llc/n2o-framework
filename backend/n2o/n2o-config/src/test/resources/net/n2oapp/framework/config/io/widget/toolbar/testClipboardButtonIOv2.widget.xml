<?xml version='1.0' encoding='UTF-8'?>
<table xmlns="http://n2oapp.net/framework/config/schema/widget-5.0">
    <toolbar>
        <clipboard-button id="btn" badge="{badge}" badge-color="{color}" datasource="table" model="resolve"
                          icon="icon" icon-position="right" color="primary" visible="true" enabled="true"
                          class="test" style="test" description="desc" tooltip-position="right"
                          src="testSrc" badge-image-position="right" badge-image-shape="square"
                          badge-position="left" badge-shape="square" badge-image="/static/candidate.png"
                          data="{snils}" type="text" message="Скопировано в буфер обмена">
            <dependencies>
                <enabling model="resolve" datasource="ref" message="Не указана дата"/>
                <visibility model="resolve" datasource="ref"/>
            </dependencies>
        </clipboard-button>
    </toolbar>
</table>
