<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-4.0" name="Первая страница">
    <regions>
        <region>
            <table id="main" auto-focus="false">
                <datasource query-id="testOpenPageTwoPathParam"/>
                <toolbar>
                    <button id="openPage" label="Открыть" datasource="main">
                        <open-page page-id="testOpenPageTwoPathParamModal" route="/:rec_id/:rec_name/open">
                            <datasources>
                                <datasource default-values-mode="defaults"/>
                            </datasources>
                            <params>
                                <path-param name="rec_id" value="{id}"/>
                                <path-param name="rec_name" value="{name}"/>
                            </params>
                        </open-page>
                    </button>
                </toolbar>
                <columns>
                    <column text-field-id="name"/>
                    <column text-field-id="type"/>
                </columns>
            </table>
        </region>
    </regions>
</page>