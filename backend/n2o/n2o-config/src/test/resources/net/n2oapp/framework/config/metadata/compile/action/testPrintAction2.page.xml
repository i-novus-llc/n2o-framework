<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-4.0" name="first" route="/page">
    <regions>
        <form id="test">
            <toolbar>
                <button id="id1">
                    <print url="/test"/>
                </button>
                <button id="id2">
                    <print url="/test2/:param1/:param2?param3=:param3">
                        <path-param name="param1" value="{field1}" model="filter" datasource="secondWgt"/>
                        <path-param name="param2" value="{field2}"/>
                        <query-param name="param3" value="{field3}" model="filter" datasource="secondWgt"/>
                    </print>
                </button>
                <button id="id3">
                    <print url="http://google.com"/>
                </button>
                <button id="id4">
                    <show-modal page-id="testPrintAction"/>
                </button>
                <button id="id5">
                    <print url="/:url"/>
                </button>
            </toolbar>
        </form>
        <form id="secondWgt">
            <toolbar>
                <button id="secWgt" model="filter" datasource="test">
                    <print url="/test/:minPrice/">
                        <path-param name="minPrice" value="{minPrice}"/>
                    </print>
                </button>
            </toolbar>
        </form>
    </regions>
</page>