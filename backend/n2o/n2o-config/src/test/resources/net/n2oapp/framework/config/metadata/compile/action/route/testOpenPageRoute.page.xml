<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0">
    <form id="form">
        <toolbar>
            <button id="btn1">
                <open-page page-id="testOpenPageRoutePage" route="/update"/>
            </button>
