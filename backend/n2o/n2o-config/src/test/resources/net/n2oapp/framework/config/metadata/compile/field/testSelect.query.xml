<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0" route="test">
    <filters>
        <eq filter-id="id" field-id="id"/>
        <contains filter-id="countries" field-id="country"/>
        <eq filter-id="search" field-id="name"/>
        <contains filter-id="name" field-id="name"/>
        <eq filter-id="ref" field-id="ref"/>
        <notEq filter-id="noRef" field-id="ref"/>
    </filters>
    <fields>
        <field id="id"/>
        <field id="image"/>
        <field id="country"/>
        <field id="name"/>
        <field id="ref" select="false"/>
    </fields>
</query>