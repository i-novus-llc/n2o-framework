<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0" name="blank" route="test">
    <fields>
        <field id="id">
            <select/>
            <filters>
                <eq filter-id="id"/>
            </filters>
        </field>
        <field id="image">
            <select/>
        </field>
        <field id="country">
            <select/>
            <filters>
                <contains filter-id="countries"/>
            </filters>
        </field>
        <field id="name">
            <select/>
            <filters>
                <eq filter-id="search"/>
                <contains filter-id="name"/>
            </filters>
        </field>
        <field id="ref">
            <filters>
                <eq filter-id="ref"/>
                <notEq filter-id="noRef"/>
            </filters>
        </field>
    </fields>
</query>