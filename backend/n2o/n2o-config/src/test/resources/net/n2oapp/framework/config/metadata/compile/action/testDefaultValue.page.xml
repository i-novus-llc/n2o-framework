<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-2.0">
    <form query-id="testShowModal">
        <fields>
            <input-text id="surname" param="name" default-value="Petr"/>
            <date-interval id="birthDate" begin-param="birthDay" >
                <default-value begin="2019-02-14T00:00:00"/>
            </date-interval>
            <input-select id="gender" query-id="testGender" param="gender_id"/>
        </fields>
    </form>
</simple-page>
