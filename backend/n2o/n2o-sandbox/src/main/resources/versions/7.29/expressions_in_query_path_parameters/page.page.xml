<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-4.0">
    <datasources>
        <datasource id="pageDs" query-id="test">
            <filters>
                <eq field-id="check" param="check"/>
            </filters>
        </datasource>
    </datasources>

    <regions>
        <form datasource="pageDs">
            <fields>
                <row>
                    <input-text id="id"/>
                    <input-text id="name"/>
                    <input-text id="check"/>
                </row>
            </fields>
        </form>
    </regions>
</page>
