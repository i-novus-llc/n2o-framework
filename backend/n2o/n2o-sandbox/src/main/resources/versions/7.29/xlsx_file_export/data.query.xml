<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list>
        <test file="data.json" operation="findAll"/>
    </list>
    <count>
        <test file="data.json" operation="count"/>
    </count>

    <filters>
        <like field-id="name" filter-id="name"/>
    </filters>

    <fields>
        <field id="id" domain="integer" sorting="true" name="Идентификатор"/>
        <field id="id_ips" sorting="true" name="Идентификатор ИПС"/>
        <field id="name" sorting="true" name="Наименование"/>
        <field id="region" domain="string" name="Регион"/>
    </fields>
</query>
