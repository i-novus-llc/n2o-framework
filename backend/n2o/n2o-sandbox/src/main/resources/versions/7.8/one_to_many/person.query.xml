<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0" object-id="person">
    <list>
        <test file="persons.json"/>
    </list>
    <filters>
        <eq filter-id="id" field-id="id"/>
    </filters>
    <fields>
        <field id="id" domain="string" sorting="true"/>
        <field id="name" select-expression="name"/>
    </fields>
</query>