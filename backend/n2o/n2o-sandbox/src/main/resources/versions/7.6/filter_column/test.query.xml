<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list>
        <test file="test.json"/>
    </list>
    <count>
        <test file="test.json" operation="count"/>
    </count>
    <filters>
        <eq filter-id="id" field-id="id"/>
        <like filter-id="name" field-id="name"/>
        <more filter-id="birthday.begin" field-id="birthday"/>
        <less filter-id="birthday.end" field-id="birthday"/>
    </filters>
    <fields>
        <field id="id"/>
        <field id="name"/>
        <field id="birthday" domain="localdate"/>
    </fields>
</query>