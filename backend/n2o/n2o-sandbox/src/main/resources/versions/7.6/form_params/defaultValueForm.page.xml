<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0">
    <form>
        <datasource query-id="detail"/>
        <fields>
            <input-text id="parent_id" label="Должен быть равен id мастер виджета на первой странице" param="parent_id" default-value="naaa"/>
            <date-interval id="birthDate" label="Начальная дата из мастер виджета на первой странице" begin-param="birthDate">
                <default-value begin="2019-02-14T00:00:00"/>
            </date-interval>
            <input-select id="gender" label="Должен быть Мужской" param="gender_id" query-id="gender"/>
        </fields>
    </form>
</simple-page>