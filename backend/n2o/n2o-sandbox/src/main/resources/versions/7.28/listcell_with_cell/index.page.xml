<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
             name="Ячейка со списком значений">
    <table selection="none">
        <datasource query-id="test"/>
        <columns>
            <column text-field-id="fio" label="Фамилия И.О."/>
            <column text-field-id="links" label="Список ссылок">
                <list label-field-id="name">
                    <link url="{link}" target="self"/>
                </list>
            </column>
            <column text-field-id="texts" label="Список текста">
                <list/>
            </column>
        </columns>
    </table>
</simple-page>