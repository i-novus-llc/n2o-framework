<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
      name="Управление фильтрацией при нажатии на Enter в фильтрах таблицы">
    <regions>
        <tabs>
            <tab name="fetch-on-enter='true'">
                <table ref-id="table">
                    <filters fetch-on-enter="true"/>
                </table>
            </tab>
            <tab name="fetch-on-enter='false'">
                <table ref-id="table">
                    <filters fetch-on-enter="false"/>
                </table>
            </tab>
        </tabs>
    </regions>
</page>