<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-3.0"
             name="Поле интервал">
    <form query-id="test">
        <fields>
            <interval-field id="num" label="Интервал">
                <begin>
                    <input-text id="beginNum" domain="integer"/>
                </begin>
                <end>
                    <input-text id="endNum" domain="integer"/>
                </end>
            </interval-field>
            <interval-field id="date" label="Дата">
                <begin>
                    <date-time id="beginDate"/>
                </begin>
                <end>
                    <date-time id="endDate"/>
                </end>
            </interval-field>
        </fields>
    </form>
</simple-page>