<?xml version='1.0' encoding='UTF-8'?>
<object xmlns="http://n2oapp.net/framework/config/schema/object-4.0">
    <operations>
        <operation id="create"
                   success-text="value={value} path_value={path_value} header_value={header_value} form_value={form_value}">
            <invocation>
                <test file="json/test.json" operation="echo"/>
            </invocation>
            <in>
                <field id="value"/>
                <field id="path_value" param="path_value"/>
                <field id="form_value"/>
                <field id="header_value" param="header_value"/>
            </in>
        </operation>
    </operations>
</object>
