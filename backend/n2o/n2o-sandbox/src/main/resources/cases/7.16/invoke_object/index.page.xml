<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-3.0"
             name="Ссылка на объект в invoke">
    <table query-id="test" object-id="table">
        <columns>
            <column text-field-id="name"/>
            <column text-field-id="type"/>
        </columns>
        <toolbar place="topLeft">
            <button id="buttonObject" label="invoke в button.object" disable-on-empty-model="false">
                <invoke object-id="button" operation-id="buttonEcho"/>
            </button>
            <button id="tableObject" label="invoke в table.object" disable-on-empty-model="false">
                <invoke operation-id="tableEcho"/>
            </button>
        </toolbar>
    </table>
</simple-page>
