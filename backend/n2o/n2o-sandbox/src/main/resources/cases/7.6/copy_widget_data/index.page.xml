<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-3.0"
      name="Копирование данных из одного виджета в другой">
    <regions>
        <region>
            <table id="table" query-id="test">
                <columns>
                    <column text-field-id="id"/>
                    <column text-field-id="name"/>
                </columns>
                <toolbar place="bottomRight">
                    <button id="btn" label="Копирование">
                        <copy mode="merge" target-widget-id="form"/>
                    </button>
                </toolbar>
            </table>
            <form id="form">
                <fields>
                    <input-text id="id"/>
                    <input-text id="name"/>
                </fields>
            </form>
        </region>
    </regions>
</page>
