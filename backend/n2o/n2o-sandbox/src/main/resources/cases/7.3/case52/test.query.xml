<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="test.json"/>
    </list>

    <fields>
        <field id="id" domain="integer">
            <select/>
            <filters>
                <eq filter-id="id"/>
                <in filter-id="ids"/>
            </filters>
        </field>
        <field id="parentId">
            <select/>
        </field>
        <field id="name">
            <select/>
            <filters>
                <like filter-id="name"/>
            </filters>
        </field>
    </fields>
</query>
