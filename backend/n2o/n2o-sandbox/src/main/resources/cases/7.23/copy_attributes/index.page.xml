<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
             name="Копирование данных из виджета в текущую/родительскую страницу">
    <form id="form">
        <fields>
            <input-text id="id"/>
            <input-text id="name"/>
        </fields>
        <toolbar>
            <button id="update" label="Открыть" disable-on-empty-model="false">
                <show-modal page-id="modal"/>
            </button>
        </toolbar>
    </form>
</simple-page>
