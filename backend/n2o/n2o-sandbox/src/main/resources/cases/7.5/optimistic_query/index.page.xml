<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-3.0"
             name="Режим optimistic запросов на сервер">
    <table query-id="test">
        <columns>
            <column text-field-id="check" label="Без optimistic" id="c1">
                <checkbox>
                    <invoke operation-id="update"/>
                </checkbox>
            </column>
            <column text-field-id="check" label="C optimistic" id="c2">
                <checkbox>
                    <invoke operation-id="update" optimistic="true"/>
                </checkbox>
            </column>
        </columns>
    </table>
</simple-page>
