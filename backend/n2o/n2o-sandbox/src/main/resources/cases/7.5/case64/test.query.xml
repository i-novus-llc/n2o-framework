<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0"
       object-id="test">
    <list>
        <test file="json/test.json"/>
    </list>

    <fields>
        <field id="id" domain="integer">
            <select/>
            <filters>
                <eq filter-id="id"/>
            </filters>
        </field>
        <field id="name">
            <select/>
            <filters>
                <eq filter-id="name"/>
            </filters>
        </field>
        <field id="ref.id" domain="integer">
            <select/>
            <filters>
                <in filter-id="refs*.id"/>
                <eq filter-id="ref.id"/>
            </filters>
        </field>
        <field id="ref.name" domain="string">
            <select/>
        </field>
    </fields>
</query>
