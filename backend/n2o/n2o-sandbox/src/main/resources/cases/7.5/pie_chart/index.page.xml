<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-3.0"
      name="Круговая диаграмма">
    <regions>
        <region>
            <chart width="730" height="250" query-id="pie">
                <pie center-x="150" center-y="120" inner-radius="50" outer-radius="80"
                     name-field-id="name" value-field-id="value" color="#82ca9d" has-label="true"/>
            </chart>
        </region>
        <line label="Круговая диаграмма c tooltip">
            <chart width="730" height="250" query-id="pie">
                <pie name-field-id="name" value-field-id="value" tooltip-field-id="tooltip"/>
            </chart>
        </line>
    </regions>
</page>
