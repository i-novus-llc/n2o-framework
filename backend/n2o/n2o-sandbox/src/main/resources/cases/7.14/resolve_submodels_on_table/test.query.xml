<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="test.json" operation="findAll"/>
    </list>

    <fields>
        <field id="id">
            <select/>
            <filters>
                <eq filter-id="id"/>
            </filters>
        </field>
        <field id="name">
            <select/>
        </field>
        <field id="gender.id">
            <select/>
            <filters>
                <eq filter-id="gender.id">gender.id:eq:gender.id</eq>
            </filters>
        </field>
        <field id="type.id">
            <select/>
            <filters>
                <eq filter-id="type.id">type.id:eq:type.id</eq>
            </filters>
        </field>
    </fields>
</query>