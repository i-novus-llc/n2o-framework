<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list>
        <test file="person.json"/>
    </list>
    <filters>
        <eq filter-id="id" field-id="id"/>
        <eq filter-id="begindate" field-id="begindate"/>
        <eq filter-id="enddate" field-id="enddate"/>
    </filters>
    <fields>
        <field id="id"/>
        <field id="name"/>
        <field id="begindate" domain="date"/>
        <field id="enddate" domain="date"/>
    </fields>
</query>