<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list>
        <test file="persons.json" operation="findAll"/>
    </list>
    <count>
        <test file="persons.json" operation="count"/>
    </count>
    <filters>
        <eq filter-id="id" field-id="id"/>
    </filters>
    <fields>
        <field id="id" domain="integer" sorting="true"/>
        <field id="name"/>
        <field id="gender"/>
        <field id="status"/>
    </fields>
</query>