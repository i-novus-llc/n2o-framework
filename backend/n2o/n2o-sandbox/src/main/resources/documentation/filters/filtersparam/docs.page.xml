        <datasource id="docs" query-id="docs">
            <filters>
                <eq field-id="person.id" param="person_id"/>
            </filters>
        </datasource>
    </datasources>
    <regions>
        <table datasource="docs">
            <columns>
                <column text-field-id="name"/>
            </columns>
        </table>
    </regions>
</page>

