<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="person.json"/>
    </list>

    <fields>
        <field id="id">
            <select/>
            <filters>
                <eq filter-id="id"/>
            </filters>
        </field>
        <field id="name">
            <select/>
        </field>
        <field id="begindate" domain="date">
            <select/>
            <filters>
                <eq filter-id="begindate"/>
            </filters>
        </field>
        <field id="enddate" domain="date">
            <select/>
            <filters>
                <eq filter-id="enddate"/>
            </filters>
        </field>
    </fields>
</query>
