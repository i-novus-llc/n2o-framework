<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0"
       object-id="user">
    <unique>
        <test file="user.json" operation="findOne"/>
    </unique>

    <fields>
        <field id="id">
            <select/>
        </field>
        <field id="gender.id">
            <select mapping="[gender]"/>
        </field>
    </fields>
</query>
