<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list>
        <test file="person.json" operation="findAll"/>
    </list>
    <count>
        <test file="person.json" operation="count"/>
    </count>

    <filters>
        <!--Фильтр на поиск подстроки в строке-->
        <like field-id="name" filter-id="name"/>
    </filters>

    <fields>
        <field id="id" domain="integer"/>
        <field id="name"/>
    </fields>
</query>

