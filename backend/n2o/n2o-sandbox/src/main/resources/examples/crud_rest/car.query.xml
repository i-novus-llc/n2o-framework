<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0"
       object-id="car">
    <list result-mapping="['_embedded.cars']" count-mapping="['page.totalElements']">
        <rest>/data/cars/search/findAll?{filters}&amp;page={page}&amp;size={limit}</rest>
    </list>
    <unique>
        <rest>/data/cars/{id}</rest>
    </unique>

    <filters>
        <eq field-id="id" filter-id="id"/>
        <eq field-id="name" filter-id="name"/>
        <more field-id="price" filter-id="minPrice">minPrice={minPrice}</more>
        <less field-id="price" filter-id="maxPrice">maxPrice={maxPrice}</less>
    </filters>

    <fields>
        <field id="id"/>
        <field id="name"/>
        <field id="price" domain="long"/>
    </fields>
</query>
