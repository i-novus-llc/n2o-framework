<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="json/test.json" operation="findAll"/>
    </list>

    <fields>
        <field id="id">
            <filters>
                <eq filter-id="id"/>
            </filters>
            <select/>
        </field>
        <field id="name">
            <select/>
        </field>
        <field id="type">
            <select/>
        </field>
        <field id="pageName">
            <select mapping="'type'+['type']"/>
        </field>
    </fields>
</query>
