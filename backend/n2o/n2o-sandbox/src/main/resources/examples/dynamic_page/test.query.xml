<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list>
        <test file="json/test.json" operation="findAll"/>
    </list>

    <filters>
        <eq field-id="id" filter-id="id"/>
    </filters>

    <fields>
        <field id="id"/>
        <field id="name"/>
        <field id="type"/>
        <field id="pageName" mapping="'type'+['type']"/>
    </fields>
</query>
