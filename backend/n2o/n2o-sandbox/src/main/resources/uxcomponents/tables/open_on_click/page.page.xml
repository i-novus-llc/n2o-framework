<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
             name="Страница записи">
    <form>
        <datasource query-id="data">
            <filters>
                <eq field-id="id" param="id"/>
            </filters>
        </datasource>
        <fields>
            <input-text id="name" label="Фамилия И.О" enabled="false"/>
            <date-time id="date" label="Дата рождения"  enabled="false"/>
        </fields>
    </form>
</simple-page>
