<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list>
        <test file="data.json"/>
    </list>
    <count>
        <test file="data.json" operation="count"/>
    </count>
    <filters>
        <eq filter-id="id" field-id="id"/>
        <like filter-id="name" field-id="name"/>
        <more filter-id="date.begin" field-id="date"/>
        <less filter-id="date.end" field-id="date"/>
    </filters>
    <fields>
        <field id="id"/>
        <field id="name"/>
        <field id="date" domain="localdate"/>
    </fields>
</query>