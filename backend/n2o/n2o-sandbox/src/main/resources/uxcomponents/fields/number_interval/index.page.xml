<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
             name="Интервал чисел">
    <form>
        <fields>
            <interval-field id="interval" no-label-block="true">
                <begin>
                    <input-text id="numFrom" domain="integer" default-value="1"/>
                </begin>
                <end>
                    <input-text id="numTo" domain="integer" default-value="10"/>
                </end>
            </interval-field>
        </fields>
    </form>
</simple-page>
