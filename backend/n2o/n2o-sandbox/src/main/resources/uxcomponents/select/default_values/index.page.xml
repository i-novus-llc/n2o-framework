<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
             name="Выпадающий список с мультирежимом с несколькими значениями по умолчанию">
    <form>
        <fields>
            <input-select
                    id="inputSelectMulti"
                    placeholder="Введите значение"
                    label="Ввод с выпадающим списком"
                    type="multi"
                    max-tag-text-length="20">
                <default-values>
                    <value id="fio2"/>
                    <value id="fio3"/>
                </default-values>
                <options>
                    <option id="fio1" name="Алексей Иванов"/>
                    <option id="fio2" name="Иван Алексеев"/>
                    <option id="fio3" name="Павел Афанасьев"/>
                    <option id="fio4" name="Петр Сергеев"/>
                </options>
            </input-select>
        </fields>
    </form>
</simple-page>
