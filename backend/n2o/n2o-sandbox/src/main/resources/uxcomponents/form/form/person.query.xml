<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <unique filters="id">
        <test file="person.json" operation="findOne"/>
    </unique>
    <filters>
        <eq filter-id="id" field-id="id"/>
    </filters>
    <fields>
        <field id="id" domain="integer"/>
        <field id="name"/>
        <field id="surname"/>
        <field id="birthday"/>
    </fields>
</query>