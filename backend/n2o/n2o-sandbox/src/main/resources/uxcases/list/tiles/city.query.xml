<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="data.json" />
    </list>
    <unique>
        <test file="data.json" operation="findOne"/>
    </unique>

    <fields>
        <field id="id">
            <select/>
            <filters>
                <eq filter-id="id"/>
            </filters>
        </field>
        <field id="name">
            <select/>
        </field>
        <field id="description">
            <select/>
        </field>
        <field id="image">
            <select/>
        </field>
        <field id="link">
            <select/>
        </field>
    </fields>
</query>
