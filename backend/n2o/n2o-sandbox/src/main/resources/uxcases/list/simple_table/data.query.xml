<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test file="data.json"/>
    </list>
    <fields>
        <field id="id">
            <select/>
        </field>
        <field id="name">
            <select/>
        </field>
        <field id="region">
            <select/>
        </field>
        <field id="population">
            <select mapping="[population]+' млн'"/>
        </field>
        <field id="area">
            <select mapping="[area]+' кв.км'"/>
        </field>
        <field id="link">
            <select/>
        </field>
    </fields>
</query>

