<access xmlns="http://n2oapp.net/framework/config/schema/access-2.0">
    <user username="user">
        <object-access object-id="testObjectAccessTransformer"/>
        <object-access object-id="testToolbarAccessTransformer" operations="*"/>
    </user>
    <role id="role">
        <object-access object-id="testObjectAccessTransformer" operations="create,update"/>
    </role>
    <role id="role_delete">
        <object-access object-id="testObjectAccessTransformer" operations="delete"/>
    </role>
    <role id="admin">
        <object-access object-id="testToolbarAccessTransformer" operations="create"/>
    </role>
    <permission id="permission">
        <object-access object-id="testObjectAccessTransformer" operations="create"/>
        <object-access object-id="testToolbarAccessTransformer" operations="*"/>
    </permission>
</access>
