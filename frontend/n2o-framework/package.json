{
  "name": "n2o-framework",
  "version": "7.29.1",
  "description": "N2O React",
  "homepage": "https://n2o.i-novus.ru/",
  "engines": {
    "node": "20.x || 22.x"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:i-novus-llc/n2o-framework.git",
    "directory": "frontend/n2o-framework"
  },
  "main": "lib/index.js",
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "dist",
    "lib",
    "src/sass"
  ],
  "keywords": [
    "react",
    "redux",
    "bootstrap",
    "components",
    "ui",
    "javascript"
  ],
  "author": "I-Novus LLC<n2o@i-novus.ru>",
  "license": "Apache-2.0",
  "scripts": {
    "build": "yarn run build:lib && yarn run build:dist",
    "build:lib": "yarn run clean:lib && yarn run compile:lib",
    "build:dist": "yarn run clean:dist && yarn run compile:dist",
    "build:local": "node scripts/build.js",
    "compile:lib": "tsc",
    "compile:lib:watch": "tsc --watch",
    "copystyles": "copyfiles -u 2 src/sass/**/* dist/styles",
    "copyfonts": "copyfiles -u 2 public/fonts/* dist/fonts",
    "compile:dist": "yarn run copystyles && yarn run copyfonts",
    "compile:dist:watch": "chokidar src/**/*.scss -c \"yarn run copystyles\"",
    "clean": "yarn run clean:lib && yarn run clean:dist && rimraf ./node_modules/",
    "clean:lib": "rimraf ./lib/",
    "clean:dist": "rimraf ./dist/",
    "tsc-clean:lib": "tsc --build --clean",
    "start": "node scripts/run.js",
    "server": "node scripts/dev-server/dev-server.js",
    "test": "jest --noStackTrace --coverage --silent ./src",
    "test:watch": "jest --watch --noStackTrace",
    "test:hook": "jest --bail --findRelatedTests",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix ./src",
    "stylelint": "stylelint ./src/**/*{.css,.scss,.sass}",
    "stylelint:fix": "stylelint --fix ./src/**/*{.css,.scss,.sass}",
    "lint": "yarn eslint && yarn stylelint"
  },
  "dependencies": {
    "@fontsource/exo-2": "^4.3.0",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@i-novus/n2o-components": "7.29.1",
    "@popperjs/core": "^2.2.1",
    "@reduxjs/toolkit": "1.6.2",
    "axios": "^1.10.0",
    "bootstrap": "~4.3.1",
    "classnames": "^2.5.1",
    "connected-react-router": "6.9.3",
    "dayjs": "^1.11.13",
    "deepmerge": "4.3.1",
    "flat": "6.0.1",
    "font-awesome": "4.7.0",
    "global": "4.4.0",
    "history": "^4.9.0",
    "i18next": "19.7.0",
    "i18next-intervalplural-postprocessor": "2.0.2",
    "invariant": "^2.2.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "numeral": "2.0.6",
    "path-to-regexp": "2.2.1",
    "print-js": "1.6.0",
    "prop-types": "15.8.1",
    "query-string": "9.2.1",
    "rc-tree": "5.13.1",
    "react-big-calendar": "1.8.7",
    "react-helmet": "6.1.0",
    "react-hotkeys": "1.0.3",
    "react-i18next": "14.1.2",
    "react-markdown": "^10.1.0",
    "react-onclickoutside": "6.13.2",
    "react-popper": "2.3.0",
    "react-redux": "7.2.6",
    "react-resize-detector": "^6.1.0",
    "react-router-dom": "5.2.0",
    "react-virtualized": "9.22.6",
    "reactstrap": "8.10.1-popper2-beta.0",
    "recharts": "^2.12.7",
    "redux": "4.1.2",
    "redux-actions": "2.6.5",
    "redux-mock-store": "1.5.4",
    "redux-saga": "1.3.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "reselect": "3.0.1",
    "sockjs-client": "^1.6.1",
    "stompjs": "^2.3.3",
    "tslib": "^2.4.0",
    "url-parse": "^1.5.10",
    "use-context-selector": "^1.4.1"
  },
  "devDependencies": {
    "@i-novus/eslint-config": "^1.0.19",
    "@types/classnames": "^2.3.1",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^28.1.3",
    "@types/lodash": "^4.14.182",
    "@types/numeral": "^2.0.2",
    "@types/query-string": "^5.1.0",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "5.3.0",
    "@types/react-virtualized": "9.22.0",
    "@types/redux-actions": "2.6.5",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.27.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "chokidar-cli": "3.0.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.15.1",
    "css-loader": "^0.28.9",
    "enzyme": "^3.9.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.57.0",
    "fetch-mock": "^6.0.0",
    "file-loader": "^1.1.6",
    "jest": "^28.1.1",
    "jest-cli": "28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "mini-css-extract-plugin": "^2.1.0",
    "mocha": "^6.2.2",
    "react-test-renderer": "^17.0.2",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "5.0.7",
    "sass-loader": "^10.2.1",
    "sinon": "^4.1.3",
    "style-loader": "^0.20.1",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^28.0.5",
    "typescript": "4.7.3"
  },
  "peerDependencies": {
    "css-animation": "*",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "redux-thunk": "*",
    "sass": "*"
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "^@snippets/(.*)$": "<rootDir>/src/components/snippets/$1",
      "^@ducks/(.*)$": "<rootDir>/src/ducks/$1",
      "fixme-todo": "HACK next 3 lines Error importing a component into jest when the package is not build in the node-modules",
      "react-markdown": "<rootDir>/test/__mocks__/fileMock.js",
      "remark-gfm": "<rootDir>/test/__mocks__/fileMock.js",
      "rehype-raw": "<rootDir>/test/__mocks__/fileMock.js"
    },
    "coverageReporters": [
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.test.{js,jsx}",
      "!src/**/*Old/*.*",
      "!src/**/*.old.{js,jsx}",
      "!**/constants/**",
      "!**/framework/**",
      "!**/tools/**",
      "!src/store.js",
      "!src/sagas.js",
      "!src/reducers.js"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "jsx",
      "tsx",
      "json"
    ],
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "ts-jest"
    }
  }
}
